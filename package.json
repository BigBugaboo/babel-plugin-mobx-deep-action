{
  "name": "@bugaboo/babel-plugin-mobx-deep-action",
  "license": "MIT",
  "version": "2.0.3",
  "description": "Allow to reduce boilerplate of writing async actions",
  "repository": "BigBugaboo/babel-plugin-mobx-deep-action",
  "author": "bugaboo <hj497304787@vip.qq.com>",
  "main": "lib/index.js",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.13.14",
    "@babel/runtime": "^7.13.10",
    "babylon": "^6.14.1",
    "mocha": "^2.2.5",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "deploy:hotfix": "npm version patch && npm publish -access public",
    "deploy:feature": "npm version minor && npm publish -access public",
    "check-types": "tsc",
    "compile": "babel src --out-dir lib --extensions '.ts'",
    "clean": "rm -rf lib",
    "build": "babel src -d lib",
    "test": "mocha --compilers js:@babel/register",
    "test:watch": "npm run test -- --watch",
    "start": "npm run clean && npm run build"
  },
  "keywords": [
    "mobx",
    "babel-plugin"
  ],
  "dependencies": {
    "core-js": "3"
  }
}
